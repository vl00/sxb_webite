@{
}
@foreach (var item in ViewBag.Data)
{
<li>
    <a href="/SchoolRank/Detail?id=@item.Id">
        <h5 class="school-name">@item.RankName</h5>
        @if(!string.IsNullOrWhiteSpace(item.Schools)){
        <p class="rank-result">@(item.Schools)等命中搜索结果</p>
        }
    </a>
</li>
}