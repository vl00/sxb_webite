@{
    Layout = null;
}
<!doctype html>
<html>
<head>
    <meta http-equiv="content-type" content="text/html" charset="utf-8" />
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="@ViewBag.FileHost_PC/css/principle/principle.css">
    <link rel="stylesheet" href="@ViewBag.FileHost_PC/css/bootstrap.css">
    <link rel="stylesheet" href="@ViewBag.FileHost_PC/css/common.css">
    @RenderSection("css", false)
</head>
<body>
    <header class="header">
        <div>
            <div class="container">
                <div class="header-bar font-12 container d-flex justify-content-between">
                    <nav class="header-nav">
                        <ul class="d-flex">
                            <li><a href="@ViewBag.SiteHost_PC" title="首页">首页</a></li>
                            <li><a href="@ViewBag.SiteHost_PC/school" title="学校">学校</a></li>
                            <li><a href="@ViewBag.SiteHost_PC/comment" title="点评">点评</a></li>
                            <li><a href="@ViewBag.SiteHost_PC/question" title="问答">问答</a></li>
                            <li><a href="@ViewBag.SiteHost_PC/schoolrank/list" title="排行">排行</a></li>
                            <li><a href="@ViewBag.SiteHost_PC/article" title="攻略">攻略</a></li>
                        </ul>
                    </nav>
                    <div class="user-message">
                        @*<a href="/Message" title="消息" class="message">消息</a>
            <a href="/" title="个人中心" class="personal-center">个人中心</a>
            <a href="/" title="帮助中心" class="help-center">帮助中心</a>*@
                        <a href="/Login?ReturnUrl=@ViewBag.ReturnUrl" title="登录" class="login">登录</a>
                        @*<a href="@ViewBag.SiteHost_PC" title="网站导航" class="web-nav">网站导航</a>*@
                    </div>
                </div>
            </div>
        </div>
        <div class="@(ViewBag.Login?"content":"")">
            <div class="container @(ViewBag.Login?"":"logo-nav")">
                <div class="sxb-icon d-flex">
                    <h1 class="font-weight-bold"><a href="@ViewBag.SiteHost_PC" title="上学帮" class="d-block">上学帮</a></h1>
                    <span>登录</span>
                </div>
            </div>
        </div>
    </header>

    @RenderBody()
<footer class="footer position-relative">
    <div class="container d-flex justify-content-between">
        <dl class="font-12">
            <dd>
                <img src="@ViewBag.FileHost_PC/imgs/qr-code.png" alt="二维码" class="qr-code float-left" />
                <p class="scanning float-right">扫描下载上学帮APP <span class="consulting">随时随地了解学校咨询</span></p>
                <!-- <p class="consulting"></p> -->
            </dd>
        </dl>
        <dl class="font-12">
            <dt class="font-15">关于我们</dt>
            <dd><a href="@ViewBag.SiteHost_PC/home/about" title="公司介绍">公司介绍</a></dd>
        </dl>
        <dl class="font-12">
            <dt class="font-15">联系我们</dt>
            <dd><a href="@ViewBag.SiteHost_PC/home/center" title="帮助中心">帮助中心</a></dd>
            <dd><a href="@ViewBag.SiteHost_PC/home/hr" title="招贤纳士">招贤纳士</a></dd>
        </dl>

        <dl class="font-12">
            <dt class="font-15">协议声明</dt>
            <dd><a href="@ViewBag.SiteHost_PC/home/principle?idx=4" title="隐私协议">隐私协议</a></dd>
            <dd><a href="@ViewBag.SiteHost_PC/home/principle?idx=3" title="免责声明">免责声明</a></dd>
            <dd><a href="@ViewBag.SiteHost_PC/home/principle?idx=2" title="服务条款">服务条款</a></dd>
        </dl>
