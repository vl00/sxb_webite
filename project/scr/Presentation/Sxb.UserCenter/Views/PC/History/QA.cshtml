@model List<PMS.UserManage.Application.ModelDto.ModelVo.QuestionVo>
@{
    Layout = null;
}
@if (Model.Count > 0)
{
    <ul class="qa-list">
        @foreach (var qa in Model)
        {
            <li>
                <div class="question flex">
                    <div class="q-icon"><span>问</span></div>
                    <a href="https://m.sxkid.com/question/questiondetail?questionid=@qa.Id">
                        <h5 class="question-title">@qa.QuestionContent</h5>
                        <span class="time">@qa.QuestionCreateTime</span>
                    </a>
                    <span class="flex round">
                        <i></i>
                        <i></i>
                        <i></i>
                    </span>
                </div>
            </li>
        }
    </ul>
}
else
{
    <div class="not-comment text-center">
        <div><i class="comment-icon"></i></div>
        <p>暂无记录</p>
    </div>
}