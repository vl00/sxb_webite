                        offset: params.offset, // SQL语句起始索引
                        page: (params.offset / params.limit) + 1, //当前页码
                        parentId: parentId,
                        type: 2,
                        date:$("#timeSearch").val()

                    };
                    return temp;
                },
                columns: [{
                    title: '昵称',
                    field: 'name'
                }, {
                    title: '手机号',
                    field: 'phone'
                }, {
                    title: '结算方式',
                    field: 'settlementType',
                }, {
                    title: '兼职人数',
                    field: 'total'
                },
                {
                    title: '操作',
                    formatter: leaderoperation,//对资源进行操作
                }]
            })
        }


