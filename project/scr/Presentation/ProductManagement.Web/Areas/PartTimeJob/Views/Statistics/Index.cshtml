@section scripts{
<script type="text/javascript">
    var adminRole = '@ViewBag._admin.Role'
    var adminId = '@ViewBag._admin.Id'
    var code = '@ViewBag.Code'

    $(function () {

        laydate.render({
            elem: '#AllSupplierTime',
            type: 'month'
        });

        laydate.render({
            elem: '#timeSearch',
            type: 'month'
        });

        laydate.render({
            elem: '#JobTimeSearch',
            type: 'month'
        });


        $(".nav").find("li[class='active']").removeClass("active")
        //$(".nav li:eq(1)").addClass("active")

       
        //校验当前账号身份，管理员、供应商、兼职领队

        // 1：兼职，2：兼职领队，3：供应商，4：审核，5：管理员
        if (adminRole == "5") {
            GetAllSupplier();
            LoadGetSysAdminQuerySupplier()
        }
        else if (adminRole == "3") {
            GetAllLeaderBysupplier(adminId)
            LoadCurrentSupplierParentTotal(adminId)
            $("#supplierCode").attr("data-code", code)
            $("#SupplierBox").hide();
            $("#leaderBox").show();
        }
        else if (adminRole == "2") {
            $("#leaderBox").hide()
            $("#jobBox").show()
            $("#SupplierBox").hide()
            $("#leaderCode").attr("data-code", code)
            GetFindAllJobEntityByLeader(adminId);
            LoadCurrentSupplierTotal(adminId,2);
        }

        $("#container").on("click", ".seeCode", function () {
            var code = $(this).attr("data-code");
            layer.alert(code, { title: "邀请码" })
        })


        //供应商时间筛选按钮
        $("#AllSupplierTimeBtn").click(function () {
            if (adminRole == 5)
            {
                GetAllSupplier()
                LoadGetSysAdminQuerySupplier()
            }
            else
            {
                GetAllLeaderBysupplier(adminId)
                LoadCurrentSupplierParentTotal(adminId)
            }
        })

        //兼职领队时间筛选框
        $("#leaderTimeSearchBtn").click(function () {
            GetAllLeaderBysupplier(adminId);
            LoadCurrentSupplierParentTotal(adminId);
        })

        //兼职领队筛选兼职筛选
        $("#JobTimeSearchBtn").click(function () {
            GetFindAllJobEntityByLeader($("#parentId").val());
            LoadCurrentSupplierTotal($("#parentId").val(),2);
        })

            //查看所有的兼职领队
            $("#container").on("click", ".SeeCurrentAllleader", function () {
                var total = $(this).attr("data-total")

                //取出当前供应商的邀请码
                var ParentSupplierCode = $(this).next().attr("data-code")
                if (total > 0)
                {
                    //将该兼职领队的供应商id赋值弹出层
                    $("#supplierCode").attr("data-code", ParentSupplierCode)
                    adminId = $(this).attr("data-id")
                    LoadCurrentSupplierParentTotal($(this).attr("data-id"))
                    GetAllLeaderBysupplier($(this).attr("data-id"))
                    $("#SupplierBox").hide();
                    $("#leaderBox").show();
                }
                else
                {
                    layer.alert("暂无人员")
                }
            })

        $("#searchBtn").click(function () {
            GetFindAllJobEntityByLeader($("#parentId").val());
            LoadCurrentSupplierTotal($("#parentId").val());
        })

            //查看所有的兼职
            $("#container").on("click", ".SeeCurrentAllJob", function () {
                var AllJob = $(this).attr("data-total")
                var code = $(this).next().attr("data-code")

                if (AllJob > 0)
                {
                    $("#leaderCode").attr("data-code",code)
                    $("#leaderBox").hide()
                    $("#jobBox").show()
                    var parentId = $(this).attr("data-id")
                    $("#parentId").val(parentId)
                    GetFindAllJobEntityByLeader(parentId);
                    LoadCurrentSupplierTotal(parentId);
                }
                else
                {
                    layer.alert("暂无人员")
                }
            })

            //查看所有的兼职
            $("#container").on("click", ".JobCommentAndAnswer", function () {
                var jobId = $(this).attr("data-id")
                layer.open({
                    type: 2,
                    area: [500 + 'px', 600 + 'px'],
                    fix: false, //不固定
                    title: '详细 - 相关人员工作记录的详细情况',
                    content: '/PartTimeJob/Statistics/GetCommentAndAnswerByJob?JobId=' + jobId +"&isExaminerAdmin=1",
                    maxmin: true,
                    btn: ["确定"]
                })
                $(".layui-layer-max").click();
            })
        })


        //获取所有的供应商
        function GetAllSupplier()
        {
            $("#AllSupplier").bootstrapTable('destroy');
            $('#AllSupplier').bootstrapTable({
                method: 'get',
                url: "/PartTimeJob/Statistics/GetEntityListByRoleType",//请求路径
                striped: true, //是否显示行间隔色
                pageNumber: 1, //初始化加载第一页
                pagination: true,//是否分页
                sidePagination: 'server',//server:服务器端分页|client：前端分页
                pageSize: 5,//单页记录数
                pageList: [5, 10, 20, 30],//可选择单页记录数
                queryParams: function (params) {//上传服务器的参数
                    var temp = {//如果是在服务器端实现分页，limit、offset这两个参数是必须的
                        limit: params.limit, // 每页显示数量
                        offset: params.offset, // SQL语句起始索引
                        page: (params.offset / params.limit) + 1, //当前页码
                        parentId: "00000000-0000-0000-0000-000000000000",
                        type: 3,
                        date:$("#AllSupplierTime").val()
                    };
                    return temp;
                },
                columns: [{
                    title: '昵称',
                    field: 'name'
                }, {
                    title: '手机号',
                    field: 'phone'
                }, {
                    title: '结算方式',
                    field: 'settlementType',
                }, {
                    title: '兼职领队人数',
                    field: 'total'
                },
                {
                    title: '操作',
                    formatter: operation,//对资源进行操作
                }]
            })
        }

        //根据兼职领队获取所有的兼职
        function GetAllLeaderBysupplier(parentId)
        {
            $("#Allleader").bootstrapTable('destroy');
            $('#Allleader').bootstrapTable({
                method: 'get',
                url: "/PartTimeJob/Statistics/GetEntityListByRoleType",//请求路径
                striped: true, //是否显示行间隔色
                pageNumber: 1, //初始化加载第一页
                pagination: true,//是否分页
                sidePagination: 'server',//server:服务器端分页|client：前端分页
                pageSize: 5,//单页记录数
